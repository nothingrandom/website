{
  "name": "website",
  "version": "2.0.0",
  "description": "Benjamin Hollway's primary online presence",
  "author": "Benjamin Hollway <benjamin@nothingrandom.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "prestart": "run-s clean 11ty:build",
    "start": "run-p develop:*",
    "prebuild": "run-p clean",
    "build": "run-s build:*",
    "build:generate": "run-s 11ty:build parcel:build",
    "develop:generate": "run-p 11ty:watch parcel:watch",
    "develop:server": "browser-sync start --config .bs-config.js --proxy localhost:1234",
    "clean": "run-p clean:*",
    "clean:cache": "rimraf .tmp",
    "clean:dist": "rimraf dist",
    "11ty:build": "eleventy  --quiet --output .tmp/parcel",
    "11ty:watch": "ELEVENTY_ENV=development eleventy  --quiet --watch --output .tmp/11ty",
    "parcel:build": "parcel build .tmp/parcel/*.html --out-dir dist --cache-dir .tmp/.cache",
    "parcel:watch": "ELEVENTY_ENV=development parcel serve .tmp/parcel/*.html --out-dir dist --cache-dir .tmp/.cache",
    "lint": "eslint --ext .js src",
    "prepublish": "npm run snyk-protect",
    "snyk-protect": "snyk protect",
    "test": "yarn lint; snyk test --severity-threshold=high"
  },
  "alias": {
    "assets": "./src/assets",
    "includes": "./src/includes",
    "data": "./src/data"
  },
  "dependencies": {
    "@11ty/eleventy": "0.8.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-brands-svg-icons": "^5.8.2",
    "@fortawesome/pro-light-svg-icons": "^5.8.2",
    "browser-reset": "^1.0.4",
    "chokidar-cli": "^1.2.2",
    "fs-extra": "^7.0.1",
    "globby": "^9.0.0",
    "luxon": "^1.13.2",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.1.7",
    "parcel-bundler": "^1.11.0",
    "rimraf": "^2.6.3",
    "snyk": "^1.189.0",
    "workbox-build": "4.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "parcel-plugin-copy-unbundled": "file:lib/parcel/parcel-plugin-copy-unbundled",
    "parcel-plugin-eleventy-sync": "file:lib/parcel/parcel-plugin-eleventy-sync",
    "parcel-plugin-nunjucks-precompile": "file:lib/parcel/parcel-plugin-nunjucks-precompile",
    "parcel-plugin-service-worker": "file:lib/parcel/parcel-plugin-service-worker",
    "sass": "^1.18.0",
    "trim-right": "^1.0.1"
  },
  "snyk": true
}
