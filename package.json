{
  "name": "website",
  "version": "2.1.10",
  "description": "Benjamin Hollway's primary online presence",
  "author": "Benjamin Hollway <benjamin@nothingrandom.com>",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "yarn lint; snyk test --severity-threshold=high --trust-policies",
    "clean": "rm -rf dist",
    "eleventy:default": "npx eleventy",
    "eleventy:watch": "npx eleventy --watch",
    "styles:prod": "node-sass src/css/ --output dist/css/ && postcss ./dist/css/*.css --replace",
    "styles:dev": "node-sass src/css/ --output dist/css/ --source-map true --source-map-contents true && postcss ./dist/css/*.css --replace",
    "scripts:prod": "webpack --mode=production",
    "scripts:dev": "webpack --mode=development",
    "serve": "light-server -c .lightserverrc",
    "dev": "ELEVENTY_ENV=development npm-run-all clean styles:dev scripts:dev eleventy:default --parallel eleventy:watch serve --print-label",
    "dev:debug": "DEBUG=* npm run dev",
    "build": "run-s clean styles:prod scripts:prod eleventy:default --print-label",
    "lint": "eslint --ext .js src",
    "postbuild:lint": "jsonlint dist/*.json -q",
    "prepublish": "yarn run snyk-protect",
    "snyk-protect": "snyk protect"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.2",
    "graphlib": "^2.1.8",
    "jsonlint": "^1.6.3",
    "light-server": "2.6.4"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.0",
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-brands-svg-icons": "^5.13.0",
    "@fortawesome/pro-duotone-svg-icons": "^5.13.0",
    "@fortawesome/pro-light-svg-icons": "^5.13.0",
    "@fortawesome/pro-regular-svg-icons": "^5.13.0",
    "@fortawesome/pro-solid-svg-icons": "^5.13.0",
    "@mightyplow/eleventy-plugin-cache-buster": "^1.1.3",
    "acorn": "^7.1.1",
    "autoprefixer": "^10.0.0",
    "babel-loader": "^8.0.6",
    "browser-reset": "^1.0.5",
    "cssnano": "^5.0.0",
    "cssnano-preset-advanced": "^5.0.0",
    "eleventy-plugin-lazyimages": "^2.0.1",
    "eleventy-plugin-reading-time": "^0.0.1",
    "lazysizes": "^5.2.2",
    "leadingzero": "^1.0.0",
    "node-fetch": "^2.6.1",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.13",
    "postcss-cli": "^8.0.0",
    "postcss-import": "13.0.0",
    "snyk": "^1.518.0",
    "webpack": "^5.1.1",
    "webpack-cli": "^4.0.0"
  },
  "snyk": true,
  "browserslist": [
    "> 0.5%",
    "Explorer >= 10",
    "last 2 versions",
    "not dead"
  ]
}
